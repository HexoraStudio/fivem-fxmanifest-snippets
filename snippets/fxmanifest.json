{
  "fxmanifest-script": {
    "prefix": "fxmanifest-script",
    "body": [
      "fx_version 'cerulean'",
      "game 'gta5'",
      "",
      "author '${1:YourName}'",
      "description '${2:Script description}'",
      "version '${3:1.0.0}'",
      "",
      "shared_scripts {",
      "  '${4:config.lua}'",
      "}",
      "",
      "client_scripts {",
      "  '${5:client/main.lua}'",
      "}",
      "",
      "server_scripts {",
      "  '${6:server/main.lua}'",
      "}",
      "",
      "--escrow_ignore {",
      "--  '${7:config.lua}',",
      "--  '${8:client/utils.lua}'",
      "--}",
      "",
      "lua54 'yes'"
    ],
    "description": "Complete FXManifest for shared/client/server Lua scripts"
  },
  "fxmanifest-mlo": {
    "prefix": "fxmanifest-mlo",
    "body": [
      "-- FXManifest for an MLO (Map Loader Object)",
      "fx_version 'cerulean'",
      "game 'gta5'",
      "",
      "description '${1:MLO Name}'",
      "",
      "this_is_a_map 'yes'",
      "",
      "files {",
      "  '${2:interiorproxies.meta}',",
      "  '${3:stream/*.ytyp}',",
      "  '${4:stream/*.ydr}',",
      "  '${5:stream/*.ybn}',",
      "  '${6:stream/*.ymap}',",
      "  '${7:other files}'",
      "}",
      "",
      "data_file 'INTERIOR_PROXY_ORDER_FILE' '${8:interiorproxies.meta}'"
    ],
    "description": "FXManifest for MLOs (map mods)"
  }
}
